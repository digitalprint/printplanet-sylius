    "printplanet/sylius-currency": "@dev",
    "printplanet/sylius-locale": "@dev",
    "printplanet/sylius-product": "@dev",
    "printplanet/sylius-repository": "@dev",
    "printplanet/sylius-taxonomy": "@dev",
